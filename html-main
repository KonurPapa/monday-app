            <script data-minify="1" src="https://billd.com/wp-content/cache/min/1/ajax/libs/angularjs/1.4.3/angular.min.js?ver=1698763484"></script>
            <script data-minify="1" src="https://billd.com/wp-content/cache/min/1/rzajac/angularjs-slider/master/dist/rzslider.js?ver=1698763484"></script>

            <script type="text/javascript">
                var myApp = angular.module("myapp", ["rzSlider"]);
                var gsp_residentialJobs = 0;
                myApp.controller("TestController", TestController);

                function TestController() {
                    var vm = this;
                    vm.priceSlider = {
                        value: 0,
                        options: {
                            id: "slider-id",
                            floor: 0,
                            ceil: 10,
                            autoHideLimitLabels: false,
                            showSelectionBar: true,
                            onChange: function(id) {
                                gsp_cresidential(vm.priceSlider.value)
                            },
                            translate: function(value) {
                                if (value == 10) return value + "+"
                                else return value
                            },

                        }
                    }
                }
            </script>
